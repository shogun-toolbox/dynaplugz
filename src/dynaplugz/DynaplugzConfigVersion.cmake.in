set(
  PACKAGE_VERSION
    "@PROJECT_VERSION@@PROJECT_VERSION_GIT@"
)

if( "${PACKAGE_VERSION}" VERSION_LESS "${PACKAGE_FIND_VERSION}" )
  set(
    PACKAGE_VERSION_COMPATIBLE
      FALSE
  )
else( "${PACKAGE_VERSION}" VERSION_LESS "${PACKAGE_FIND_VERSION}" )
  set(
    PACKAGE_VERSION_COMPATIBLE
      TRUE
  )
  if( "${PACKAGE_VERSION}" VERSION_EQUAL "${PACKAGE_FIND_VERSION}" )
    set(
      PACKAGE_VERSION_EXACT
        TRUE
    )
  endif( "${PACKAGE_VERSION}" VERSION_EQUAL "${PACKAGE_FIND_VERSION}" )
endif( "${PACKAGE_VERSION}" VERSION_LESS "${PACKAGE_FIND_VERSION}" )

set( DYNAPLUGZ_SOVER     "@PROJECT_SOVER@" )
set( DYNAPLUGZ_SOVER_CUR "@PROJECT_SOVER_CUR@" )
set( DYNAPLUGZ_SOVER_REV "@PROJECT_SOVER_REV@" )
set( DYNAPLUGZ_SOVER_AGE "@PROJECT_SOVER_AGE@" )
